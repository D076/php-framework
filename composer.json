{
  "name": "d076/php-framework",
  "description": "Simple PHP framework",
  "license": "MIT",
  "authors": [
    {
      "name": "Dmitry Titov",
      "email": "dezmond076@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-pdo": "*"
  },
  "require-dev": {
    "symfony/var-dumper": "^6.3",
    "laravel/pint": "^1.13",
    "phpunit/phpunit": "^10.0.7"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "D076\\PhpFramework\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "D076\\PhpFramework\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
